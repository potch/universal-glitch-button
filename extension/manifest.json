{
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Glitch Project Info",
    "default_popup": "panel.html"
  },
  "id": "universal-glitch-button@glitch.com",
  "browser_specific_settings": {
    "gecko": {
      "id": "universal-glitch-button@glitch.com"
    }
  },
  "name": "Universal Glitch Button",
  "description": "View info about a glitch project",
  "homepage_url": "https://universal-glitch-button.glitch.me",
  "icons": {
    "16": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "permissions": ["activeTab", "http://*.glitch.me", "https://*.glitch.me", "https://glitch.com/*"],
  "version": "3.6.0",
  "manifest_version": 2
}
